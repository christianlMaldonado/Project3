  "bugs": {
    "url": "https://github.com/christianlMaldonado/Project3/issues"
  },
  "homepage": "https://github.com/christianlMaldonado/Project3#readme",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "if-env": "^1.0.4",
    "express": "^4.17.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-router-dom": "^5.1.2"
  }
}
